{
  "stackName": "parrot-rp",
  "clusterPrefix": "parrot-rp-cp",
  "hostMachineDnsServer": "192.168.104.47",
  "workerHostCount": 0,
  "dockerMachineWorkerOptions": {
    "engineLabel": "role=worker"
  },
  "dockerMachineMasterOptions": {
    "engineLabel": "role=master"
  },
  "dockerMachineDriverOptions": {
    "engineInsecureRegistry": "docker-registry.parrot-rp.keyw:5000",
    "driver": "vmwarevsphere",
    "vmwarevsphereUsername": "root",
    "vmwarevspherePassword": "run2walk!",
    "vmwarevsphereDatastore": "datastore1",
    "vmwarevsphereCpuCount": 2,
    "vmwarevsphereDiskSize": 40000,
    "vmwarevsphereMemorySize": 4096,
    "vmwarevsphereVcenter": "vmware.parrot-rp.keyw"
  },
  "dockerComposeYaml": {
    "version": "3.4",
    "networks": {
      "net": null
    },
    "volumes": {
      "pg-datavolume": null
    },
    "services": {
      "pgadmin4": {
        "image": "docker-registry.parrot-rp.keyw:5000/pgadmin4:0.07.12",
        "environment": [
          "HOPE=SpringsEternal"
        ],
        "deploy": {
          "replicas": 1,
          "restart_policy": {
            "condition": "on-failure"
          },
          "placement": {
            "constraints": [
              "node.role == manager"
            ]
          },
          "labels": [
            "traefik.enable=true",
            "traefik.port=5050",
            "traefik.backend=pgadmin4",
            "traefik.frontend.rule=Host: pgadmin4.parrot-rp.keyw"
          ]
        },
        "ports": [
          "5050:5050"
        ],
        "volumes": [
          {
            "type": "volume",
            "source": "pg-datavolume",
            "target": "/var/lib/postgresql/data",
            "volume": {
              "nocopy": false
            }
          }
        ],
        "networks": [
          "net"
        ]
      },
      "postgres": {
        "image": "docker-registry.parrot-rp.keyw:5000/postgres:0.07.12",
        "environment": [
          "POSTGRES_PASSWORD=password"
        ],
        "deploy": {
          "replicas": 1,
          "restart_policy": {
            "condition": "on-failure"
          },
          "placement": {
            "constraints": [
              "node.role == manager"
            ]
          },
          "labels": [
            "traefik.enable=true",
            "traefik.port=5432",
            "traefik.backend=postgres",
            "traefik.frontend.rule=Host: postgres.parrot-rp.keyw"
          ]
        },
        "ports": [
          "5432:5432"
        ],
        "volumes": [
          {
            "type": "volume",
            "source": "pg-datavolume",
            "target": "/var/lib/postgresql/data",
            "volume": {
              "nocopy": false
            }
          }
        ],
        "networks": [
          "net"
        ]
      }
    }
  }
}

