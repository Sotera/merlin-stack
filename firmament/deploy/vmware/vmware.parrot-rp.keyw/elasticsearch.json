{
  "stackName": "parrot-rp",
  "clusterPrefix": "parrot-rp-cp",
  "hostMachineDnsServer": "192.168.104.47",
  "workerHostCount": 0,
  "dockerMachineWorkerOptions": {
    "engineLabel": "role=worker"
  },
  "dockerMachineMasterOptions": {
    "engineLabel": "role=master"
  },
  "dockerMachineDriverOptions": {
    "engineInsecureRegistry": "docker-registry.parrot-rp.keyw:5000",
    "driver": "vmwarevsphere",
    "vmwarevsphereUsername": "root",
    "vmwarevspherePassword": "run2walk!",
    "vmwarevsphereDatastore": "datastore1",
    "vmwarevsphereCpuCount": 2,
    "vmwarevsphereDiskSize": 40000,
    "vmwarevsphereMemorySize": 4096,
    "vmwarevsphereVcenter": "vmware.parrot-rp.keyw"
  },
  "dockerComposeYaml": {
    "version": "3.4",
    "networks": {
      "net": null
    },
    "volumes": {
      "elasticsearch-datavolume": null
    },
    "services": {
      "elasticsearch-head": {
        "image": "docker-registry.parrot-rp.keyw:5000/elasticsearch-head:0.07.12",
        "environment": [
          "HOPE=SpringsEternal"
        ],
        "deploy": {
          "replicas": 1,
          "restart_policy": {
            "condition": "on-failure"
          },
          "placement": {
            "constraints": [
              "node.role == manager"
            ]
          },
          "labels": [
            "traefik.enable=true",
            "traefik.port=9100",
            "traefik.backend=elasticsearch-head",
            "traefik.frontend.rule=Host: elasticsearch-head.parrot-rp.keyw"
          ]
        },
        "ports": [
          "9100:9100"
        ],
        "volumes": [
          {
            "type": "volume",
            "source": "elasticsearch-datavolume",
            "target": "/usr/share/elasticsearch/data",
            "volume": {
              "nocopy": false
            }
          }
        ],
        "networks": [
          "net"
        ]
      },
      "elasticsearch": {
        "image": "docker-registry.parrot-rp.keyw:5000/elasticsearch:0.07.12",
        "environment": [
          "HOPE=SpringsEternal"
        ],
        "deploy": {
          "replicas": 1,
          "restart_policy": {
            "condition": "on-failure"
          },
          "placement": {
            "constraints": [
              "node.role == manager"
            ]
          },
          "labels": [
            "traefik.enable=true",
            "traefik.port=9200",
            "traefik.backend=elasticsearch",
            "traefik.frontend.rule=Host: elasticsearch.parrot-rp.keyw"
          ]
        },
        "ports": [
          "9200:9200",
          "9300:9300"
        ],
        "networks": [
          "net"
        ]
      }
    }
  }
}

