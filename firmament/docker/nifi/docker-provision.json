{
  "stackName": "merlin",
  "clusterPrefix": "merlin",
  "workerHostCount": 0,
  "dockerMachineMasterOptions": {
    "engineLabel": "role=master"
  },
  "dockerMachineDriverOptions": {
    "driver": "virtualbox",
    "virtualboxCpuCount": 2,
    "virtualboxMemory": 8092,
    "engineInsecureRegistry": "52.0.211.45:5000"
  },
  "dockerComposeYaml": {
    "version": "3.3",
    "networks": {
      "net": null
    },
    "services": {
      "nifi": {
        "image": "192.168.99.100:5000/nifi:0.08.05",
        "environment": [
          "HOPE=SpringsEternal"
        ],
        "deploy": {
          "replicas": 1,
          "restart_policy": {
            "condition": "on-failure"
          }
        },
        "networks": [
          "net"
        ]
      }
    }
  }
}
